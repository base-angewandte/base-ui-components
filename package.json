{
  "name": "base-ui-components",
  "description": "base Angewandte Component Library",
  "version": "0.5.1",
  "homepage": "https://base.uni-ak.ac.at",
  "author": "baseAngewandte <basemail@uni-ak.ac.at>",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/base-angewandte/base-ui-components.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/base-ui-components.min.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prepublishOnly": "npm run build",
    "dev": "vue-cli-service serve src/",
    "dev:docs": "webpack-dev-server --progress --config build/webpack.dev.conf.js",
    "dev:lib": "webpack --config build/webpack.lib.conf.js --watch --progress --hide-modules & node server.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && node-sass --output-style expanded src/styles/lib.scss | cleancss -o dist/lib.min.css",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:components": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format components",
    "build:docs": "vue-styleguidist build",
    "svg": "npx vsvg -s ./src/static/icons/ -t ./src/assets/icons/ --es6",
    "updateindex": "node build/generate-index-files.js",
    "updateindex:all": "node build/generate-index-files.js --force true",
    "styleguide": "vue-styleguidist server",
    "postversion": "./postversion.sh"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "hls.js": "^0.13.1",
    "lazysizes": "^5.2.2",
    "leaflet": "^1.6.0",
    "normalize.css": "^8.0.1",
    "swiper": "^5.3.1",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue2-datepicker": "^3.5.0",
    "vue2-leaflet": "^2.5.2",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.7.4",
    "@babel/traverse": "^7.9.6",
    "@babel/types": "^7.9.6",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-service": "^4.4.6",
    "autoprefixer": "^9.7.6",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.18.3",
    "chalk": "^3.0.0",
    "clean-css-cli": "^4.3.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-vue": "^6.1.2",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^2.1.0",
    "postcss-cli": "^7.1.1",
    "postcss-csso": "^4.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "rimraf": "^2.7.1",
    "rollup": "^2.23.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass-loader": "^7.3.1",
    "semver": "^5.7.1",
    "shelljs": "^0.7.6",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-styleguidist": "^4.22.2",
    "vue-svgicon": "^3.2.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  }
}
