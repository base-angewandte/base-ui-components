{
  "name": "base-ui-components",
  "version": "1.0.0-beta.8",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --port 9001",
    "dev": "node build-utils/update-index-file.js && npm-run-all --parallel serve dev:lib",
    "dev:lib": "vue-cli-service build --target lib --dest dist/lib  --name base-ui-components ./src/index.js --watch --no-clean",
    "build": "npm-run-all --parallel build:docs build:lib",
    "build:docs": "vue-cli-service build --dest dist/docs",
    "build:lib": "node build-utils/build-lib.js",
    "build:wc": "vue-cli-service build --target wc-async --dest dist/lib-wc --name base-ui-components 'src/components/*/*.vue'",
    "lint": "vue-cli-service lint",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "axios": "^0.19.0",
    "body-scroll-lock": "^2.6.4",
    "hls.js": "^0.12.4",
    "normalize.css": "^8.0.1",
    "vue-click-outside": "^1.0.7",
    "vue2-datepicker": "^2.12.0",
    "vuedraggable": "^2.23.1",
    "vue-svgicon": "^3.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "doctrine": "^3.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "extract-comments": "^1.1.0",
    "fs-extra": "^8.1.0",
    "loader-utils": "^1.2.3",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.10",
    "vue-cli-plugin-styleguidist": "^3.13.1",
    "vue-svg-loader": "^0.12.0",
    "vue-svgicon": "^3.2.2",
    "vue-template-compiler": "^2.5.21"
  },
  "main": "dist/lib/base-ui-components.umd.js",
  "author": "baseAngewandte <basemail@uni-ak.ac.at>",
  "homepage": "https://base.uni-ak.ac.at",
  "description": "base Angewandte Component Library",
  "repository": {
    "type": "git",
    "url": "https://github.com/base-angewandte/ui-components.git"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "packages/base-ui-components"
  ]
}
