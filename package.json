{
  "name": "base-ui-components",
  "version": "0.0.51",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --port 9001",
    "dev": "npm-run-all --parallel serve dev:lib",
    "dev-new": "node build-utils/update-index-file.js && vue-cli-service serve --port 9001",
    "dev:lib": "vue-cli-service build --target lib --dest dist/lib  --name base-ui-components ./src/lib.js --watch --no-clean",
    "build": "npm-run-all --parallel build:docs build:lib",
    "build-new": "node build-utils/build-lib.js",
    "build:docs": "vue-cli-service build --dest dist/docs",
    "build:lib": "vue-cli-service build --target lib --dest dist/lib --name base-ui-components ./src/lib.js",
    "build:wc": "vue-cli-service build --target wc-async --dest dist/lib-wc --name base-ui-components 'src/components/*/*.vue'",
    "lint": "vue-cli-service lint",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "postversion": "git push && git push --tags && git checkout master && git merge develop && git push && git push --tags && npm publish"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "body-scroll-lock": "^2.6.1",
    "hls.js": "^0.12.4",
    "normalize.css": "^8.0.1",
    "vue-click-outside": "^1.0.7",
    "vue2-datepicker": "^2.12.0",
    "vue-svg-loader": "^0.12.0",
    "vue-svgicon": "^3.2.2",
    "vuedraggable": "^2.17.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "fs-extra": "^8.1.0",
    "loader-utils": "^1.2.3",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.0.1",
    "style-resources-loader": "^1.2.1",
    "vue": "^2.5.21",
    "vue-cli-plugin-style-resources-loader": "^0.1.3",
    "vue-cli-plugin-styleguidist": "^3.13.1",
    "vue-template-compiler": "^2.5.21"
  },
  "main": "dist/lib/base-ui-components.umd.js",
  "author": "baseAngewandte <basemail@uni-ak.ac.at>",
  "description": "base Angewandte Component Library",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "files": [
    "dist/lib"
  ]
}
