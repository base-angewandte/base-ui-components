{
  "name": "base-ui-components",
  "version": "0.5.1",
  "private": false,
  "description": "base Angewandte Component Library",
  "author": "baseAngewandte <basemail@uni-ak.ac.at>",
  "scripts": {
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && npm run build:transform-css:all",
    "build:components": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format components && npm run build:transform-css",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es && npm run build:transform-css:all",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs && npm run build:transform-css:all",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife && npm run build:transform-css:all",
    "build:watch": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --watch",
    "build:transform-css": "build/transform-css.sh",
    "build:transform-css:all": "build/transform-css.sh -a",
    "svg": "npx vsvg -s ./src/static/icons/ -t ./src/assets/icons/ --es6 && node ./build/update-svg-icon-files.js",
    "updateindex": "node build/generate-index-files.js",
    "updateindex:all": "node build/generate-index-files.js --force true",
    "postversion": "./postversion.sh",
    "prepublishOnly": "npm run build",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/base-ui-components.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "hls.js": "^0.13.1",
    "lazysizes": "^5.2.2",
    "leaflet": "^1.7.1",
    "normalize.css": "^8.0.1",
    "swiper": "^5.4.5",
    "vue": "^2.6.12",
    "vue-click-outside": "^1.1.0",
    "vue-svgicon": "^3.2.8",
    "vue2-datepicker": "^3.6.2",
    "vuedraggable": "^2.24.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "autoprefixer": "^9.8.6",
    "axios": "^0.19.0",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.18.3",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^7.0.2",
    "dart-sass": "^1.25.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-vue": "^6.2.2",
    "express": "^4.17.1",
    "minimist": "^1.2.5",
    "postcss-cli": "^7.1.2",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "rollup": "^2.28.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "source-map-loader": "^1.1.0",
    "vue-cli-plugin-styleguidist": "~4.32.3",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "homepage": "https://base.uni-ak.ac.at",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/base-angewandte/base-ui-components.git"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
