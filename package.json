{
  "name": "base-ui-components",
  "version": "2.0.4",
  "private": false,
  "description": "base Angewandte Component Library",
  "author": "baseAngewandte <basemail@uni-ak.ac.at>",
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && npm run build:transform-css",
    "build:transform-css": "sh build/transform-css.sh",
    "build:watch": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --watch",
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "postversion": "./postversion.sh",
    "prepublishOnly": "npm run build",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "cross-env NODE_ENV=build vue-cli-service styleguidist:build",
    "svg": "node ./build/generate-svg-sprite-sheet.js",
    "test:unit": "vue-cli-service test:unit",
    "updateindex": "node build/generate-index-files.js",
    "updateindex:all": "node build/generate-index-files.js --force true",
    "prepare": "husky install",
    "changelog": "gulp changelog"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/base-ui-components.min.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "body-scroll-lock": "^2.6.4",
    "hls.js": "^1.2.8",
    "lazysizes": "^5.3.0",
    "leaflet": "^1.8.0",
    "leaflet-responsive-popup": "^1.0.0",
    "leaflet.markercluster": "^1.5.3",
    "normalize.css": "^8.0.1",
    "swiper": "^6.8.4",
    "vue-click-outside": "^1.1.0",
    "vue2-datepicker": "^3.10.4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-unit-jest": "~5.0.8",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/vue2-jest": "^27.0.0",
    "autoprefixer": "^9.8.8",
    "axios": "^0.21.4",
    "babel-jest": "^27.5.1",
    "clean-css-cli": "^4.3.0",
    "core-js": "^3.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.7.1",
    "gulp": "^4.0.2",
    "gulp-conventional-changelog": "^2.0.35",
    "husky": "^8.0.2",
    "jest": "^27.1.0",
    "minimist": "^1.2.6",
    "postcss-cli": "^7.1.2",
    "postcss-discard-comments": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-url": "^8.0.0",
    "react": "^18.2.0",
    "rollup": "^2.71.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^5.1.9",
    "sass": "^1.50.1",
    "sass-loader": "^10.2.1",
    "source-map-loader": "^1.1.3",
    "svgo": "^3.0.2",
    "svgstore-cli": "^2.0.1",
    "vue": "^2.7.14",
    "vue-cli-plugin-styleguidist": "^4.54.4",
    "yargs": "^17.6.2"
  },
  "peerDependencies": {
    "vue": "^2.7.14"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "homepage": "https://base.uni-ak.ac.at",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/base-angewandte/base-ui-components.git"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "lint-staged": {
    "*.{js,vue}": "npm run lint"
  }
}
